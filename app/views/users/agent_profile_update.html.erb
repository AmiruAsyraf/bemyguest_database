

<section id="update_agent" class="home-section bg-white">
<h1>Update Agent</h1>
<label>First Name :</label>
<input type="text" name="txt_first_name" class="form-control">
<br />

<label>Last Name :</label>
<input type="text" name="txt_last_name" class="form-control">
<br />

<label>Email :</label>
<input type="email" name="txt_email" class="form-control">
<br/>

<label>Password :</label>
<input type="password" name="txt_password" class="form-control">
<br/>

<label>Address :</label>
  <textarea class="form-control" rows="5" name="txt_address"></textarea>
 <br />

<label>Post Code :</label>
<input type="text" name="postcode" class="form-control">
<br />

<label>State :</label>
<input type="text" name="state" class="form-control">
<br />

<label>Country :</label>
<select name="slc_country" class="form-control">
  <option value="malaysia">Malaysia</option>
  <option value="japan">Japan</option>
  <option value="korea">Korea</option>
  <option value="china">China</option>
</select>
<br />

<label>Gender :</label>
<select name="slc_gender" class="form-control">
  <option value="male">Male</option>
  <option value="female">Female</option>
</select>
<br />

<label>Religion :</label>
<select name="slc_religion" class="form-control">
  <option value="islam">Islam</option>
  <option value="hindu">Hindu</option>
</select>
<br />

<label>Race :</label>
<select name="slc_race" class="form-control">
  <option value="malay">Malay</option>
  <option value="indian">Indian</option>
</select>
<br />

<label>Phone Number :</label>
<input type="text" name="txt_phone_number" class="form-control">
<br />

<label>Skype :</label>
<input type="text" name="txt_skype" class="form-control">
<br />

<label>Role :</label>
<select name="slc_role" class="form-control">
  <option value="admin">Admin</option>
  <option value="traveller">Traveller</option>
  <option value="agent">Agent</option>
</select>
<br />

<label>Medical Condition :</label>
<input type="text" name="txt_medical_condition" class="form-control">
<br />

<button type="submit" class="btn btn-default">Submit</button>

</section>

